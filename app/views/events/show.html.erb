<%= render 'sidenav' %>

<div class="page_content">

<p id="notice"><%= notice %></p>

<p>
  <b>Event Name:</b>
  <%= @event.name %>
</p>

<p>
  <b>Description:</b>
  <%= @event.description %>
</p>

<p>
  <b>Has Alert?</b>
  <%= @event.alert_id ? 'yes' : 'no' %>
</p>


<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Back', events_path %>

</div>
